<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="@dimen/p10"
    tools:context=".M_l001">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/mL_b001"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="@dimen/p20"
            android:background="@color/colorPrimary"
            android:text="@string/mL_b001"
            android:textColor="@color/White"
            android:textSize="@dimen/fontsize"
            android:textStyle="bold" />

        <Button
            android:id="@+id/mL_b002"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="@dimen/p20"
            android:background="@color/colorPrimary"
            android:text="@string/mL_b002"
            android:textColor="@color/White"
            android:textSize="@dimen/fontsize"
            android:textStyle="bold" />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/mL_b003"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="@dimen/p20"
            android:background="@color/colorPrimary"
            android:text="@string/mL_b003"
            android:textColor="@color/White"
            android:textSize="@dimen/fontsize"
            android:textStyle="bold"
            style="?android:attr/buttonBarButtonStyle" />

        <Button
            android:id="@+id/mL_b004"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="@dimen/p20"
            android:background="@color/colorPrimary"
            android:text="@string/mL_b004"
            android:textColor="@color/White"
            android:textSize="@dimen/fontsize"
            android:textStyle="bold"
            style="?android:attr/buttonBarButtonStyle" />




    </TableRow>

    <TextView
        android:id="@+id/mL_t001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p50"
        android:gravity="end"
        android:text="@string/name1"
        android:textColor="@color/Blue"
        android:textSize="@dimen/fontsize" />

</LinearLayout>